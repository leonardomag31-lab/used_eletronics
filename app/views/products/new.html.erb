<!-- new.html.erb -->
<h1>Novo Produto</h1>
<%= render "form", product: @product %>

<%= form_with model: @product, local: true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :images %>
  <%= f.file_field :images, multiple: true %>

  <%= f.submit "Salvar" %>
<% end %>
